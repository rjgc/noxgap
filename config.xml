<?xml version='1.0' encoding='utf-8'?>
<widget id="com.newoxygen.pos" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <author>Adobe</author>  
    <name>PhoneGap Build App</name>  
    <preference name="windows-identity-name" value="PhonegapBuild.PhoneGapBuild" />  
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <engine name="windows" spec="~4.4.3" />
</widget>
